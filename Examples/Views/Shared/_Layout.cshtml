<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SeedPacket Examples</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body><div class="mainContent">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="@Html.ActiveToggle("Home","Index")">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li class="@Html.ActiveToggle("DataSource","Index")">@Html.ActionLink("DataSource", "Index", "DataSource")</li>
                    <li class="@Html.ActiveToggle("Extending","Index")">@Html.ActionLink("Extending", "Index", "Extending")</li>
                    <li class="@Html.ActiveToggle("Rows","Index")">@Html.ActionLink("25k Rows", "Index", "Rows")</li>
                    <li class="@Html.ActiveToggle("Rules","Index")">@Html.ActionLink("Rules List", "Index", "Rules")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year Will Crowther - SeedPacket</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/ScriptApp")
    @RenderSection("scripts", required: false)
</div>
</body>
</html>
