@using SeedPacket.Examples.Logic.Models
@model List<User>

<div id="rowResults" class="side-scroll bottom-margin">

	<div class="example-bar">
		<h4 class="example-title">Users Example</h4>
		<span class="example-scrolls">Table Scrolls Left / Right</span>
	</div>

    <table id="home-simple-example-table" 
		class="table table-striped table-collapse-sm">
        <thead>

            <tr>
                <th>UserId</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserId</td>
                    <td>@(!string.IsNullOrEmpty(user.FirstName) ? user.FirstName : "---")</td>
                    <td>@user.LastName</td>
                    <td>@user.Email</td>
                </tr>
            }
        </tbody>
    </table>

</div>





