<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale = 1.0,maximum-scale = 1.0" />
    <title>SeedPacket Examples</title>
    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/grid.css" rel="stylesheet" asp-append-version="true" />
</head>
<body>
    <div class="background-gradation"></div>
    <div class="background-gradation-right-corner"></div>
    <div class="mainContent">

        <partial name="_NavBar" />

        <div class="container body-content">
            @RenderBody()

            <hr />
            <footer class="clearfix">
                <p>&copy; @DateTime.Now.Year Will Crowther - SeedPacket</p>
            </footer>
        </div>


    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <partial name="_Widget" />
</body>
</html>
