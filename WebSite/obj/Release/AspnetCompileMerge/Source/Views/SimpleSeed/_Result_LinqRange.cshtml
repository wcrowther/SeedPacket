@using MvcExamples.Models
@{ 
    var userList = Enumerable.Range(1, 15)
    .Select(n => new User {
        UserId = n,
        FirstName = string.Format("FirstName" + n),
        LastName = string.Format("LastName" + n),
        Email = string.Format("FLastName" + n + "@test.com"),
        Created = DateTime.Now.AddMonths(n)
    }).ToList();
}
<table class="table table-striped topSpace">
    <thead>
        <tr>
            <th>UserId</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
            <th>Created</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in userList)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.Created.ToString("MM/dd/yyyy")</td>
            </tr>
        }
    </tbody>
</table>


