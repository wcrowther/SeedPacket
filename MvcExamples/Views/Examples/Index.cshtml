}
<div class="row">
    <div class="col-md-12">
        <h3>Examples</h3>
    </div>
    <div class="col-md-12" id="rowResults">
        <table class="table table-striped topSpace">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>FirstName</th>
                    <th>LastName</th>
                    <th>Email</th>
                    <th>Investments</th>
                    <th>State</th>
                    <th>Created</th>
                    <th>New Field</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var example in exampleList)
                {
                    <tr>
                        <td>@example.Id</td>
                        <td>@example.FirstName</td>
                        <td>@example.LastName</td>
                        <td>@example.Email</td>
                        <td>@example.Investments</td>
                        <td>@example.State</td>
                        <td>@example.Created</td>
                        <td>@example.Properties</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
